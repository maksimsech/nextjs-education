{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "baseUrl": "src",
        "lib": ["ES2021"],
        "module": "ESNext",
        "target": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"]
}